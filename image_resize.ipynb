    "# 이미지 크기 줄이기\n",
    "from PIL import Image\n",
    "\n",
    "# 원본 이미지 열기\n",
    "image = Image.open('ktlogo2.png')\n",
    "\n",
    "# 이미지 크기 조정\n",
    "resized_image = image.resize((280, 60))\n",
    "\n",
    "# 조정된 이미지 저장\n",
    "resized_image.save('ktlogo.png')"
   ]
